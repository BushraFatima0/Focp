//TO ACCEPT TWO INTEGER NUMBERS AND SWAP THEM USING DIFFERENT METHODS

#include <stdio.h>
int main() 
{
    // ques 4
    int a,b,t,orig_a,orig_b;
    printf("Enter 2 Numbers:\n");
    scanf("%d %d",&a,&b);
    orig_a=a;
    orig_b=b;
    //method 1
    t=a;
    a=b;
    b=t;
    printf("Method 1\nAfter swapping a=%d and b=%d\n",a,b);
    
    //method 2
    a=orig_a;
    b=orig_b;
    a=a+b;
    b=a-b;
    a=a-b;
    printf("Method 2\nAfter swapping a=%d, b=%d\n",a,b);
    
    //method 3
    a=orig_a;
    b=orig_b;
    a=a*b;
    b=a/b;
    a=a/b;
    printf("Method 3\nAfter swapping a=%d, b=%d\n",a,b);


    return 0;
}
