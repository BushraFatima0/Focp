//CHECK WHETHER SCORE IS EVEN OR ODD
#include <stdio.h>
int main() 
{
    //ques7
    int marks[5]={67,75,86,69,92};
    for(int i=0;i<5;i++)
    {
        if(marks[i]%2==0)
        printf("Score %d is Even\n",marks[i]);
        else
        printf("Score %d is odd\n",marks[i]);
    }
    return 0;
}
