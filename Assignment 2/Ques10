//TO VOUNT PRIME NUMBERS IN AN ARRAY
#include <stdio.h>
int main() 
{
    //ques10
    int count=0;
    int a[5]={67,75,86,69,92};
    printf("Prime Numbers are:\n");
    for(int i=0;i<5;i++)
    {
        for(int j=2;j<a[i];j++)
        {
            if(a[i]%j==0)
            count++;
        }
        if (count==0)
        printf("%d\n",a[i]);
    }
    return 0;
}
